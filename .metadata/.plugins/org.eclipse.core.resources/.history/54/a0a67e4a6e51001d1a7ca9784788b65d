package entidade;

public abstract class Entidade {
	public Integer id; 
	
	
	public String toString(int x) 
	{
		String str = Integer.toString(x);
		return str;
	}
	public String toString(float x) 
	{
		String str = Float.toString(x);
		return str;
	}
	public String toString(Double x) 
	{
		String str = Double.toString(x);
		return str;
	}
	
	public abstract String getNomeLivro();
	public abstract void setNomeLivro(String nomeLivro);
	//MÃ©todos da classe EmprestimoEntidade
	public abstract String getMatriculaAluno();
	public abstract void setMatriculaAluno(String matriculaAluno);
}
