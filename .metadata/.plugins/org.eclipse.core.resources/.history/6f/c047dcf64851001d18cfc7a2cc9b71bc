package visao;

public class LivroVisao extends Visao{
	public void MenuLivroVisao() {
		int opcao = -1;
		while(opcao != 0) 
		{
			System.out.println("1 - Cadastrar Livro");
			System.out.println("2 - Visualizar Livro");
			System.out.println("0 - Retornar");
			
			opcao = ParseInt(Console.readLine());
		}
		
	}
}
