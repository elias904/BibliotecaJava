package Fabrica;


import entidade.LivroEntidade;

public interface Fabrica<Type> {
	//MÃ©todos abstrato para serem sobscritos nas subclasses
	    abstract Type createEntidade(String packageType);
	    abstract Type createPersistencia(String packageType);
	    abstract Type createControlador(String packageType);
	    abstract Type createVisao(String packageType);

}
